{
  "manifest_version": 3,
  "name": "CSP Issue Repro",
  "version": "0.0.1",
  "description": "Repro of CSP issue",
  "content_scripts": [
    {
      "matches": ["https://example.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["module.js"],
      "matches": ["https://example.com/*"],
      "use_dynamic_url": true
    }
  ],
  "permissions": [],
  "optional_permissions": [],
  "host_permissions": []
}
